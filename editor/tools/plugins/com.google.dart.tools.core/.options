#
# Internal options for the Dart Compiler
#
# To run Dart Editor with custom options,
# copy one or more of these settings to an options file
# then launch Dart Editor from the command line with 
#
#     -debug /path/to/options/file
#
com.google.dart.tools.core/debug/build = false
com.google.dart.tools.core/debug/dartlib = false
com.google.dart.tools.core/debug/metrics = false
com.google.dart.tools.core/debug/warmup = false
com.google.dart.tools.core/debug/verbose = false
com.google.dart.tools.core/experimental/filesview = false

com.google.dart.tools.core/trace/artifactProvider = false

########################################################################
# These options allow you to use the Frog compiler rather than DartC

# Set true for automatically building with Frog ...
com.google.dart.tools.core/build/frog = false

# Set true for using Frog during "Generate Optimized Javascript..."
com.google.dart.tools.core/deploy/frog = false

# Set true for debugging communication with Frog server
com.google.dart.tools.core/debug/frog = false

########################################################################
# These options allow you to override the libraries used during DartC compilation
#   com.google.dart.tools.core/platform/name
#     sets the "name" used when locating libraries
#     "compiler" for DartC
#     "frog" for Frog libraries
#     "runtime" for VM libraries
#   com.google.dart.tools.core/libraries/embedded
#     a comma separate list of dart libraries that should be implicitly imported 
#     other than dart:core
#   com.google.dart.tools.core/libraries/path
#     the path to the "libraries" directory 
#       this can be an absolute path or relative to the Dart Editor installation directory
#     the libraries directory has the format
#        sdk/lib/
#          core/
#            core_compiler.dart
#            core_frog.dart
#            core_runtime.dart
#            ... other core source files and sub directories ...
#          coreimpl/
#            ... etc...
#          dom/
#            dom.dart  <--- no platform specific version thus no _<platform-name> suffix
#            .. other dom files and subdirectories ...
#          ... other library directories ...
#

# Using the default Dart Editor DartC libraries ...
com.google.dart.tools.core/platform/name = compiler
com.google.dart.tools.core/libraries/embedded = 
com.google.dart.tools.core/libraries/path = libraries

